# Fireflies to Obsidian Sync Tool Environment Variables
# Copy this file to .env and fill in your actual values

# Fireflies API Configuration
FIREFLIES_API_KEY=your_fireflies_api_key_here
FIREFLIES_API_URL=https://api.fireflies.ai/graphql

# Obsidian Configuration
OBSIDIAN_VAULT_PATH=/path/to/your/obsidian/vault
OBSIDIAN_FOLDER_NAME=Fireflies

# Application Settings
POLLING_INTERVAL_SECONDS=15
LOG_LEVEL=INFO
ENABLE_NOTIFICATIONS=true

# Test Mode (optional)
# Set to a specific meeting ID to test with one meeting
TEST_MEETING_ID=
